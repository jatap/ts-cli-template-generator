{
  "name": "ts-cli-template-generator",
  "version": "1.0.0",
  "description": "TypeScript Cli Template Generator",
  "author": "Julio Antúnez Tarín <julio.antunez.tarin@gmail.com>",
  "license": "ISC",
  "keywords": [
    "javascript",
    "js",
    "node",
    "ts",
    "typescript",
    "cli"
  ],
  "main": "src/cli.ts",
  "bin": {
    "ts-cli-template-generator": "src/cli.js"
  },
  "scripts": {
    "***": "<------------------ build -------------------------------------->",
    "commit": "git cz",
    "release": "standard-version --no-verify -t ''"
  },
  "devDependencies": {
    "commitizen": "^4.0.3",
    "cz-customizable": "^6.2.0",
    "standard-version": "^7.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jatap/ts-cli-template-generator.git"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "cz-customizable.js"
    }
  },
  "engines": {
    "node": "10.15.3",
    "npm": "6.4.1"
  }
}
